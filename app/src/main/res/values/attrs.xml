<resources>
    <attr name="drawable" format="color|reference"/>

    <declare-styleable name="TitleView">
        <attr name="text" format="string"/>
        <attr name="dimension" format="dimension"/>
        <attr name="color" format="color"/>
        <attr name="drawable"/>
    </declare-styleable>
    <declare-styleable name="RotateView">
        <attr name="drawable"/>
        <attr name="rotation" format="integer"/>
    </declare-styleable>
    <declare-styleable name="ScrollChildSwipeRefreshLayout">
        <attr name="refreshing" format="boolean" />
    </declare-styleable>
</resources>
